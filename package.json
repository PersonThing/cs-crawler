{
  "name": "cs-crawler",
  "version": "1.0.0",
  "description": "simple js game",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:client": "vite --config ./client/vite.config.js",
    "dev:server": "nodemon ./server/src/server.js",
    "dev:textures": "node build-textures.js --watch",
    "textures": "node build-textures.js",
    "dev": "concurrently \"npm run dev:textures\" \"npm run dev:client\" \"npm run dev:server\"",
    "build": "vite build --config ./client/vite.config.js",
    "start": "node ./server/src/server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "^4.0.1",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "express": "^4.21.0",
    "heap": "^0.2.7",
    "pixi-filters": "^6.0.5",
    "pixi.js": "^8.4.1",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0"
  }
}
